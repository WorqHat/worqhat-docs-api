{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "WorqHat Docs",
  "description": "WorqHat API Documentation - The official API server for WorqHat platform. This API provides endpoints to manage workflows, retrieve metrics, and interact with the WorqHat ecosystem.",
  "colors": {
    "primary": "#12A7D3",
    "light": "#81D7F7",
    "dark": "#1b0a66"
  },
  "favicon": "/W.svg",
  "logo": {
    "light": "/logo/logo-light.png",
    "dark": "/logo/logo-dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://worqhat.com/demo"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://worqhat.app"
    }
  },
  "integrations": {
    "intercom": {
      "appId": "or2gpwjd"
    }
  },
  "styling": {
    "codeblocks": "system",
    "eyebrows": "breadcrumbs"
  },
  "background": {
    "decoration": "windows"
  },
  "api": {
    "openapi": "openapi.yaml",
    "playground": {
      "display": "interactive"
    },
    "examples": {
      "languages": ["javascript", "curl", "python", "go", "java", "typescript"]
    }
  },
  "footer": {
    "socials": {
      "x": "https://x.com/worqhat",
      "github": "https://github.com/worqhat",
      "linkedin": "https://linkedin.com/company/worqhat",
      "instagram": "https://instagram.com/worqhat",
      "youtube": "https://www.youtube.com/@worqhat"
    }
  },
  "contextual": {
    "options": ["copy", "view", "chatgpt", "claude"]
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
