{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "aspen",
  "name": "WorqHat Docs",
  "description": "WorqHat API Documentation - The official API server for WorqHat platform. This API provides endpoints to manage workflows, retrieve metrics, and interact with the WorqHat ecosystem.",
  "colors": {
    "primary": "#12A7D3",
    "light": "#81D7F7",
    "dark": "#1b0a66"
  },
  "favicon": "/W.svg",
  "logo": {
    "light": "/logo/logo-light.png",
    "dark": "/logo/logo-dark.png"
  },
  "navbar": {
    "links": [
      {
        "label": "Support",
        "href": "https://worqhat.com/demo"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://worqhat.app"
    }
  },
  "integrations": {
    "intercom": {
      "appId": "or2gpwjd"
    }
  },
  "styling": {
    "codeblocks": "system",
    "eyebrows": "breadcrumbs"
  },
  "background": {
    "decoration": "windows"
  },
  "api": {
    "openapi": "openapi.yaml"
  },
  "footer": {
    "socials": {
      "x": "https://x.com/worqhat",
      "github": "https://github.com/worqhat",
      "linkedin": "https://linkedin.com/company/worqhat",
      "instagram": "https://instagram.com/worqhat",
      "youtube": "https://www.youtube.com/@worqhat"
    }
  },
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Introduction",
            "pages": ["get-started", "quickstart", "rate-limit"]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
                      {
            "group": "System",
            "pages": [
              "api-reference/system/health",
              "api-reference/system/info"
            ]
          },
          {
            "group": "Database",
            "pages": [
              "api-reference/database/query",
              "api-reference/database/insert",
              "api-reference/database/update",
              "api-reference/database/delete",
              "api-reference/database/natural-language-query"
            ]
          },
          {
            "group": "Workflows",
            "pages": [
              "api-reference/workflows/metrics"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "API Reference",
          "icon": "terminal",
          "href": "https://docs.worqhat.com"
        },

        {
          "anchor": "Contact us",
          "icon": "envelope",
          "href": "https://worqhat.com/contact-us"
        }
      ]
    }
  }
}